<h1>Create A New Heroine</h1>


<div>
<%= form_with model: @heroine do |f| %>

<ul>
  <label>Name</label>
  <%= f.text_field :name %>
</ul>

<ul>
  <label>Super Name</label>
  <%= f.text_field :super_name %>
</ul>

<ul>
<label>Assign A Unique Power</label>
<%= f.collection_select :power_id, Power.all, :id, :name, include_blank: true %>
</ul>

<%= f.submit %>
<% end %>

</div>
