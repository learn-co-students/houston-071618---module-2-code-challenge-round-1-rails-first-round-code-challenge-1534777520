<h1>Editing Heroine</h1>

<%= render 'form', heroine: @heroine %>

<%= link_to 'Show', @heroine %> |
<%= link_to 'Back', heroines_path %>
<%= form_for @heroine do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :super_name %>:
  <%= f.text_field :super_name %><br />

  <%= f.label "Power" %>:
  <%= f.collection_select :power_id, @powers, :id, :name %>

  <%= f.submit %>
<% end %>