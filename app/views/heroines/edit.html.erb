<h1>Update Heroine</h1>
<br>
<%= form_with model: @heroine do |form| %>
  <%= form.label :name %>:
  <%= form.text_field :name %><br />

  <%= form.label :super_name %>:
  <%= form.text_field :super_name %><br />
  
  <%= form.label :power %>:
  <%= form.collection_select(:power_id, Power.all, :id, :name) %><br />


  <%= form.submit %>
<% end %>
