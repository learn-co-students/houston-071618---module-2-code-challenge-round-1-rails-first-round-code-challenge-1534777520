<%= link_to "View All Powers", powers_path %> | <%= link_to "View All Heroines", heroines_path %>

<h2>All Heroines</h2>

<%= form_tag(heroines_path method: :get) do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag "Search" %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>


<%= link_to "Create New Heroine", new_heroine_path %>
