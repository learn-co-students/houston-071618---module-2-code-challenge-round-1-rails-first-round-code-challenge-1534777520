<h2>All Heroines</h2>

<%= form_tag("/heroines", method: "get") do %>
  <b><%= label_tag(:search, "Search By Superpower:") %></b>
  <br>
  <%= text_field_tag(:search) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
